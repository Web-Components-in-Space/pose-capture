{
  "name": "pose-capture",
  "private": false,
  "version": "0.2.0",
  "description": "A set of components to perform Tensorflow.js pose detection on hands, face, and body. Provides a player to perform visualization overlays while source video or webcam is playing.",
  "main": "pose-capture.js",
  "module": "pose-capture.js",
  "type": "module",
  "files": [
    "/baseplayer.*",
    "/events.*",
    "/video-element.*",
    "/playbackevent.*",
    "/tensorflow",
    "/ui",
    "/bodypix-video.*",
    "/facelandmark-video.*",
    "/handpose-video.*",
    "/posedetection-video.*",
    "/videopose-element.*",
    "/keyframeevent.*",
    "/abstractvisualizer.*",
    "/visualization-canvas.*",
    "/pose-player.*",
    "/pose-capture.*",
    "/libs"
  ],
  "scripts": {
    "build": "tsc",
    "lib-prebundle": "./node_modules/.bin/rollup -c lib-prebundle/tensorflow-cfg.js",
    "build:watch": "tsc --watch",
    "clean": "rimraf my-element.{d.ts,d.ts.map,js,js.map} test/my-element.{d.ts,d.ts.map,js,js.map} test/my-element_test.{d.ts,d.ts.map,js,js.map}",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "docs": "npm run docs:clean && npm run build && npm run analyze && npm run docs:build && npm run docs:assets && npm run docs:gen",
    "docs:clean": "rimraf docs",
    "docs:gen": "eleventy --config=.eleventy.cjs",
    "docs:gen:watch": "eleventy --config=.eleventy.cjs --watch",
    "docs:build": "npm run lib-prebundle && rollup -c --file docs/pose-capture.bundled.js",
    "docs:assets": "cp node_modules/prismjs/themes/prism-okaidia.css docs/",
    "docs:serve": "wds --root-dir=docs --node-resolve --watch",
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\"",
    "analyze:watch": "cem analyze --litelement --globs \"src/**/*.ts\" --watch",
    "serve": "wds --watch",
    "serve:prod": "MODE=prod npm run serve",
    "test": "npm run test:dev && npm run test:prod",
    "test:dev": "wtr",
    "test:watch": "wtr --watch",
    "test:prod": "MODE=prod wtr",
    "test:prod:watch": "MODE=prod wtr --watch",
    "checksize": "rollup -c ; cat pose-capture.bundled.js | gzip -9 | wc -c ; rm pose-capture.bundled.js"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "tensorflow",
    "lit"
  ],
  "author": "Ben Farrell",
  "license": "BSD-3-Clause",
  "dependencies": {
    "lit": "^2.2.4"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "@custom-elements-manifest/analyzer": "^0.5.7",
    "@open-wc/testing": "^3.1.5",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@tensorflow-models/body-pix": "^2.2.0",
    "@tensorflow-models/face-landmarks-detection": "0.0.3",
    "@tensorflow-models/hand-pose-detection": "^0.0.1",
    "@tensorflow-models/pose-detection": "0.0.6",
    "@tensorflow/tfjs-backend-webgl": "^3.13.0",
    "@tensorflow/tfjs-converter": "^3.13.0",
    "@tensorflow/tfjs-core": "^3.13.0",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@web/dev-server": "^0.1.31",
    "@web/test-runner": "^0.13.27",
    "@web/test-runner-playwright": "^0.8.8",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "eslint": "^8.15.0",
    "koa-range": "^0.3.0",
    "lit-analyzer": "^1.2.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.73.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-summary": "^1.4.3",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "~4.6.4"
  },
  "customElements": "custom-elements.json"
}
